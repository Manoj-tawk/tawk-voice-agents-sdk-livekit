{
  "name": "@livekit-voice-agent/backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --project tsconfig.prod.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,js}\" --ignore-pattern '**/*.test.ts'",
    "lint:fix": "eslint --fix \"**/*.{ts,js}\" --ignore-pattern '**/*.test.ts'",
    "format": "prettier --write \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "test": "node --test --import tsx",
    "test:watch": "node --test --watch --import tsx",
    "dev": "tsx src/agent.ts dev",
    "download-files": "pnpm run build && node dist/agent.js download-files",
    "start": "node dist/agent.js start"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.18.11",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "@livekit/agents": "^1.0.21",
    "@livekit/agents-plugin-deepgram": "^1.0.22",
    "@livekit/agents-plugin-elevenlabs": "^1.0.22",
    "@livekit/agents-plugin-livekit": "^1.0.21",
    "@livekit/agents-plugin-openai": "^1.0.22",
    "@livekit/agents-plugin-silero": "^1.0.21",
    "@livekit/noise-cancellation-node": "^0.1.9",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "dotenv": "^17.2.3",
    "zod": "^3.25.76"
  }
}
